{
  "name": "base-client-demo",
  "version": "0.0.1",
  "description": "Base Client React App with a Nodejs hosting by Express",
  "scripts": {
    "postinstall": "node remove-bitcore-lib-dep.js && node utils/del.js TEMP-base-client-js node_modules/bitclave-base node_modules/@types/bitclave-base && git clone --branch multiprice --depth 1 https://github.com/bitclave/base-client-js.git TEMP-base-client-js && node utils/copy.js TEMP-base-client-js/example/public/base-lib/bitclave-base node_modules/bitclave-base/ && node utils/copy.js TEMP-base-client-js/example/public/base-lib/@types/bitclave-base node_modules/@types/bitclave-base/ && node utils/del.js TEMP-base-client-js"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "remove-bitcore-lib-dep.js",
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "keywords": [
    "Bitclave",
    "Base",
    "Business",
    "JS",
    "Library",
    "CAT"
  ],
  "author": "Bitclave",
  "license": "MIT",
  "homepage": "https://github.com/bitclave/base-business-js#readme",
